<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#E64A19"/> <!-- Color al presionar -->
            <corners android:radius="8dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#FF5722"/> <!-- Color normal -->
            <corners android:radius="8dp"/>
        </shape>
    </item>
</selector>